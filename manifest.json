{
  "name": "CrazyFonts",
  "description": "Get the Font name of any element!",
  "version": "2.2",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["cursor.css"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "assets/logox128.png"
  },
  "icons": {
    "16": "assets/logox16.png",
    "48": "assets/logox48.png",
    "128": "assets/logox128.png"
  }
}
